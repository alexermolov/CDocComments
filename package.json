{
    "name": "comments",
    "version": "1.0.5",
    "publisher": "AlexCoderCorp",
    "engines": {
        "vscode": "^1.12.2"
    },
    "displayName": "C-family Documentation Comments",
    "description": "Generate C-family documentation comments for /// or /**",
    "categories": [
        "Other"
    ],
    "galleryBanner": {
        "color": "#4F87FF",
        "theme": "light"
    },
    "main": "./out/src/extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "C-family Documentation Comments configuration",
            "properties": {
                "docomment.activateOnEnter": {
                    "type": "boolean",
                    "default": false,
                    "description": "Press the Enter key to activate a command."
                }
            }
        }
    },
    "activationEvents": [
        "onLanguage:csharp",
        "onLanguage:c",
        "onLanguage:cpp"
    ],
    "keywords": [
        "C-family Documentation Comments",
        "C#",
        "C++",
        "C"
    ],
    "dependencies": {},
    "devDependencies": {
        "typescript": "^2.3.2",
        "vscode": "^1.1.0",
        "tslint": "^5.2.0",
        "istanbul": "^0.4.5",
        "coveralls": "^2.13.1",
        "mocha": "^10.2.0",
        "mocha-lcov-reporter": "^1.3.0",
        "@types/node": "^7.0.18",
        "@types/mocha": "^2.2.41"
    },
    "extensionDependencies": [],
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "watch": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "test": "node ./node_modules/vscode/bin/test",
        "coverage_travis": "./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- -R spec --ui tdd ./out/test/**/*.js",
        "coverage": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec --ui tdd ./out/test/**/*.js",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "icon": "images/docomment.png",
    "license": "MIT",
    "homepage": "https://github.com/AlexCoderCorp/CDocComments#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/AlexCoderCorp/CDocComments"
    },
    "bugs": {
        "url": "https://github.com/AlexCoderCorp/CDocComments/issues"
    }
}
